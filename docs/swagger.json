                    }
                }
            }
        },
        "main.ChatsGet": {
            "type": "object",
            "properties": {
                "chats": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/main.Chat"
                    }
                }
            }
        },
        "main.ErrorGet": {
            "type": "object",
            "properties": {
                "errorMessage": {
                    "type": "string"
                }
            }
